<launch>
    <include file="$(find-pkg-share ichthus_launch)/launch/taeho/ichthus_lgsvl_taeho.launch.py">
    </include>

    <include file="$(find-pkg-share ichthus_launch)/launch/taeho/ichthus_map_taeho.launch.py">
    </include>

    <include file="$(find-pkg-share ichthus_launch)/launch/taeho/ichthus_system_taeho.launch.py">
    </include>

    <include file="$(find-pkg-share ichthus_launch)/launch/taeho/ichthus_sensing_taeho.launch.py">
    </include>

    <arg name ="lidar_channel" default="64"/>
    <arg if="$(eval &quot; '$(var lidar_channel)' =='16'&quot;)" name="base_name" default="vlp-16"/>
    <arg if="$(eval &quot; '$(var lidar_channel)' =='64'&quot;)" name="base_name" default="hdl-64"/>

    <arg name="lidar_apollo_instance_segmentation_param_path" default="$(find-pkg-share ichthus_launch)/param/lidar_apollo_instance_segmentation_$(var base_name).param.yaml"/>

    <include file="$(find-pkg-share ichthus_launch)/launch/taeho/ichthus_perception_taeho.launch.py">
      <arg name='lidar_apollo_instance_segmentation_param_path' value="$(var lidar_apollo_instance_segmentation_param_path)" />
    </include>

    <include file="$(find-pkg-share ichthus_launch)/launch/taeho/ichthus_planning_taeho.launch.py">
    </include>
</launch>
